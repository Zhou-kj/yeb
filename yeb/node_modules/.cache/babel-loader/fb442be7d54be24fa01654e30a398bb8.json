  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    name: 'SalSobCfg',\r\n    data() {\r\n        return {\r\n            emps: [],\r\n            currentPage: 1,\r\n            size: 10,\r\n            total: 0,\r\n        }\r\n    },\r\n    mounted() {\r\n        this.initEmps()\r\n    },\r\n    methods: {\r\n        sizeChange(size) {\r\n            this.size = size\r\n            this.initEmps()\r\n        },\r\n        currentChange(page) {\r\n            this.currentPage = page\r\n            this.initEmps()\r\n        },\r\n        initEmps() {\r\n            this.getRequest('/salary/sobcfg?currentPage='+this.currentPage+'&size='+this.size).then(resp => {\r\n                if (resp) {\r\n                    this.emps = resp.data\r\n                    this.total = resp.total\r\n                }\r\n            })\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style lang=\"\">\r\n    \r\n</style>"]},"metadata":{},"sourceType":"module"}